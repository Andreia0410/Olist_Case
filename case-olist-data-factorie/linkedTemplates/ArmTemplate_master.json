{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "case-olist-data-factorie"
		},
		"olist_data_lake_linked_service_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'olist_data_lake_linked_service'"
		},
		"olist_linked_service_databricks_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'olist_linked_service_databricks'"
		},
		"olist_sql_linked_service_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'olist_sql_linked_service'"
		},
		"olist_data_lake_linked_service_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://caseoliststorageaccount2.dfs.core.windows.net/"
		},
		"olist_linked_service_databricks_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0717-190449-ygaf4d1r"
		},
		"olist_pipeline_all_trigger1_properties_pipeline_olist_sql_all_parameters_cw_items": {
			"type": "array",
			"defaultValue": [
				{
					"source": {
						"table": "olist_customers_dataset"
					},
					"destination": {
						"fileName": "dbo.olist_tbcustomers_dataset.csv"
					}
				},
				{
					"source": {
						"table": "olist_geolocation_dataset"
					},
					"destination": {
						"fileName": "dbo.olist_tbgeolocation_dataset.csv"
					}
				},
				{
					"source": {
						"table": "olist_order_items_dataset"
					},
					"destination": {
						"fileName": "dbo.olist_tborder_items_dataset.csv"
					}
				},
				{
					"source": {
						"table": "olist_order_payments_dataset"
					},
					"destination": {
						"fileName": "dbo.olist_tborder_payments_dataset.csv"
					}
				},
				{
					"source": {
						"table": "olist_order_reviews_dataset"
					},
					"destination": {
						"fileName": "dbo.olist_tborder_reviews_dataset.csv"
					}
				},
				{
					"source": {
						"table": "olist_orders_dataset"
					},
					"destination": {
						"fileName": "dbo.olist_tborders_dataset.csv"
					}
				},
				{
					"source": {
						"table": "olist_products_dataset"
					},
					"destination": {
						"fileName": "dbo.olist_tbproducts_dataset.csv"
					}
				},
				{
					"source": {
						"table": "olist_sellers_dataset"
					},
					"destination": {
						"fileName": "dbo.olist_tbsellers_dataset.csv"
					}
				},
				{
					"source": {
						"table": "product_category_name_translation"
					},
					"destination": {
						"fileName": "dbo.product_tbcategory_name_translation.csv"
					}
				}
			]
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "case-olist-data-factorie_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"olist_data_lake_linked_service_accountKey": {
						"value": "[parameters('olist_data_lake_linked_service_accountKey')]"
					},
					"olist_linked_service_databricks_accessToken": {
						"value": "[parameters('olist_linked_service_databricks_accessToken')]"
					},
					"olist_sql_linked_service_connectionString": {
						"value": "[parameters('olist_sql_linked_service_connectionString')]"
					},
					"olist_data_lake_linked_service_properties_typeProperties_url": {
						"value": "[parameters('olist_data_lake_linked_service_properties_typeProperties_url')]"
					},
					"olist_linked_service_databricks_properties_typeProperties_existingClusterId": {
						"value": "[parameters('olist_linked_service_databricks_properties_typeProperties_existingClusterId')]"
					},
					"olist_pipeline_all_trigger1_properties_pipeline_olist_sql_all_parameters_cw_items": {
						"value": "[parameters('olist_pipeline_all_trigger1_properties_pipeline_olist_sql_all_parameters_cw_items')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}