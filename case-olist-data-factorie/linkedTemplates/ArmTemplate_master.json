{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Nome do Data Factory",
			"defaultValue": "case-olist-data-factorie"
		},
		"olist_data_lake_linked_service_accountKey": {
			"type": "secureString",
			"metadata": "Cadeia de caracteres segura para 'accountKey' de 'olist_data_lake_linked_service'"
		},
		"olist_sql_linked_service_connectionString": {
			"type": "secureString",
			"metadata": "Cadeia de caracteres segura para 'connectionString' de 'olist_sql_linked_service'"
		},
		"olist_data_lake_linked_service_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://caseoliststorageaccount2.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI para a conta de armazenamento contendo os modelos do ARM vinculados"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Token SAS para a conta de armazenamento contendo modelos do ARM vinculados"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "case-olist-data-factorie_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"olist_data_lake_linked_service_accountKey": {
						"value": "[parameters('olist_data_lake_linked_service_accountKey')]"
					},
					"olist_sql_linked_service_connectionString": {
						"value": "[parameters('olist_sql_linked_service_connectionString')]"
					},
					"olist_data_lake_linked_service_properties_typeProperties_url": {
						"value": "[parameters('olist_data_lake_linked_service_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}